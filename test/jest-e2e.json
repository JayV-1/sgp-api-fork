{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/user/(.*)": "<rootDir>/../libs/user/src/$1",
    "@app/user": "<rootDir>/../libs/user/src",
    "@app/project/(.*)": "<rootDir>/../libs/project/src/$1",
    "@app/project": "<rootDir>/../libs/project/src",
    "@app/agile/(.*)": "<rootDir>/../libs/agile/src/$1",
    "@app/agile": "<rootDir>/../libs/agile/src",
    "@app/auth/(.*)": "<rootDir>/../libs/auth/src/$1",
    "@app/auth": "<rootDir>/../libs/auth/src",
    "@app/commons/(.*)": "<rootDir>/../libs/commons/src/$1",
    "@app/commons": "<rootDir>/../libs/commons/src",
    "@app/security/(.*)": "<rootDir>/../libs/security/src/$1",
    "@app/security": "<rootDir>/../libs/security/src"
  }
}